            var map_ab6fe88b9bcab39708a287b69735a2b3 = L.map(
                "map_ab6fe88b9bcab39708a287b69735a2b3",
                {
                    center: [20.0, 0.0],
                    crs: L.CRS.EPSG3857,
                    zoom: 2,
                    zoomControl: true,
                    preferCanvas: false,
